<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= gravatar_for user, size: 52 %>
      <%= link_to user.name, user %>
      <% if current_user.admin? && !current_user?(user) %>
    	| <%= link_to "удалить", user, method: :delete, data: { confirm: "Вы уверены?" } %>
    <% end %>
    </li>
  <% end %>
</ul>